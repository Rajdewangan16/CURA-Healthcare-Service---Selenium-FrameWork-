<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="errorHandling"></include>
		</run>
	</groups>
	<listeners>
		<listener class-name="curaHealthCare.testComponents.Listeners"></listener>
	</listeners>
	<test thread-count="5" name="Test">
		<classes>
			<class name="curaHealthCare.tests.MakeAppointmentTest" />
			<class name="curaHealthCare.tests.LoginTest" />
			<class name="curaHealthCare.tests.ErrorValidationsTest" />
		</classes>
	</test>
</suite>
